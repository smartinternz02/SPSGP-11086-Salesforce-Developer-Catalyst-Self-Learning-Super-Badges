public class RandomContactFactory {
    public static List<Contact> generateRandomContacts(Integer i,String l){
        List<Contact> c = new List<Contact>();
        for(Integer j = 1;j<= i ;j++){
            String a = 'Test '+j;
            Contact temp = new Contact(FirstName=a,LastName=l);
            c.add(temp);
        }
        return c;
    }
}
